-form_for(@association) do |f|
  #form-entry
    %fieldset
      %legend 
        %strong
          Editing Association
      = error_messages_for :association
      %div.form-fields
        = render :partial => "associations/form", :locals => { :f=>f }
      = image_submit_tag 'buttons/global/btn-update.png', :class => 'submit'
      = link_to 'Show', @association
      = link_to 'Back', associations_path
