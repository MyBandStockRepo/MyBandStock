<%= form_for(@share_code) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :key %><br />
    <%= f.text_field :key %>
  </div>
  <div class="field">
    <%= f.label :share_code_group_id %><br />
    <%= f.text_field :share_code_group_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
