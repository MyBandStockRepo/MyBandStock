<h1>Listing api_users</h1>

<table>
  <tr>
    <th>Api key</th>
    <th>Secret key</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @api_users.each do |api_user| %>
  <tr>
    <td><%= api_user.api_key %></td>
    <td><%= api_user.secret_key %></td>
    <td><%= link_to 'Show', api_user %></td>
    <td><%= link_to 'Edit', edit_api_user_path(api_user) %></td>
    <td><%= link_to 'Destroy', api_user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

