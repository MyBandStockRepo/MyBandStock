<%= form_for(@api_user) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :api_key %><br />
    <%= f.text_field :api_key %>
  </div>
  <div class="field">
    <%= f.label :secret_key %><br />
    <%= f.text_field :secret_key %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
